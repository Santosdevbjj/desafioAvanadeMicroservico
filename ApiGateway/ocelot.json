{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/produto",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "estoqueservice",
          "Port": 8081
        }
      ],
      "UpstreamPathTemplate": "/estoque/produto",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ]
    },
    {
      "DownstreamPathTemplate": "/api/produto/{id}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "estoqueservice",
          "Port": 8081
        }
      ],
      "UpstreamPathTemplate": "/estoque/produto/{id}",
      "UpstreamHttpMethod": [ "GET", "PUT", "DELETE" ]
    },
    {
      "DownstreamPathTemplate": "/api/pedido",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "vendasservice",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/vendas/pedido",
      "UpstreamHttpMethod": [ "GET", "POST" ]
    },
    {
      "DownstreamPathTemplate": "/api/pedido/{id}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "vendasservice",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/vendas/pedido/{id}",
      "UpstreamHttpMethod": [ "GET" ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://apigateway:8080"
  }
}
